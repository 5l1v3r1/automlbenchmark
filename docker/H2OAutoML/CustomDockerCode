# Install prerequisites for H2O
RUN apt-get update
RUN apt-get install -y openjdk-8-jdk
RUN pip3 install --no-cache-dir numpy==1.15.0
RUN pip3 install --no-cache-dir scipy==1.1.0
RUN pip3 install --no-cache-dir scikit-learn==0.19.2
RUN pip3 install --no-cache-dir pandas
RUN pip3 install --no-cache-dir requests
RUN pip3 install --no-cache-dir tabulate
RUN pip3 install --no-cache-dir future
RUN pip3 install --no-cache-dir colorama>=0.3.8
RUN pip3 install --no-cache-dir h2o==3.20.0.5
RUN pip3 install --no-cache-dir liac-arff==2.3
EXPOSE 54321
EXPOSE 54322
ENV start_call="python3 run_h2oautoml.py"
